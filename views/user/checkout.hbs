  function couponValidator(total) {
    totalAmount = document.getElementById('totalAmount').text
    couponId = document.getElementById('couponId').value

    $.ajax({
      url: '/couponValidation',
      data: {
        couponId: couponId,
        total: total


      },

      method: 'post',
      success: (response) => {
        console.log(response);
        if (response.message == "success") {

          document.getElementById("couponPresent").style.visibility = 'visible'
          document.getElementById("couponPresents").style.visibility = 'visible'
          document.getElementById("couponPresentss").style.visibility = 'visible'
          document.getElementById('couponPresent').style.color = 'green';
          $("#discountAmount").text(response.coupons.discountAmount)
          $("#newTotalAmount").text(response.newTotal)
        }
        else if (response.message == 'couponExpired') {
          document.getElementById('couponPresent').style.visibility = 'visible';
          document.getElementById('couponPresent').style.color = 'red';
          document.getElementById('couponPresent').innerHTML = '&#10060; Coupon has expired';
        }
        else if (response.message == 'couponUsed') {
          document.getElementById('couponPresent').style.visibility = 'visible';
          document.getElementById('couponPresent').style.color = 'red';
          document.getElementById('couponPresent').innerHTML = '&#10060; Coupon already used ';
        }
        else if (response.message == 'couponMin') {
          document.getElementById('couponPresent').style.visibility = 'visible';
          document.getElementById('couponPresent').style.color = 'red';
          document.getElementById('couponPresent').innerHTML = '&#10060; Please add amount to your cart ';
        }
        else if (response.message == 'couponInvalid') {
          document.getElementById('couponPresent').style.visibility = 'visible';
          document.getElementById('couponPresent').style.color = 'red';
          document.getElementById('couponPresent').innerHTML = '&#10060; Coupon code is invalid';
        }

      }
    });
  }

